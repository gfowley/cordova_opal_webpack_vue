<?xml version='1.0' encoding='utf-8'?>
<widget id="net.iqeo.CordovaOpalWebpackVueTemplate" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>net.iqeo.cordova-opal-webpack-vue-template</name>
    <description>Cordova/Opal/Webpack/Vue Template</description>
    <author email="dev@iqeo.net" href="http://iqeo.net">Iqeo</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/before_prepare.js" type="before_prepare" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
